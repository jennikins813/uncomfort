<h1>User Profile Page</h1>

<%= form_for @profile do |f| %>
	
	<div class="field">
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</div>

	<div class="field">
		<%= f.label :photo %><br>
		<%= f.text_field :photo %>
	</div>


	<div class="field">
		<%= f.label :bio %><br>
		<%= f.text_area :bio %>
	</div>

	<div class="field">
		<%= f.label :website %><br>
		<%= f.text_field :website %>
	</div>
	<div class="actions">
		<%= f.submit %>
	</div>
<% end %>
